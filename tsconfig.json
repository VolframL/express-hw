{
	"env": {
		"node": true,
		"es2021": true
	},
	"compilerOptions": {
		"module": "NodeNext",
		"allowSyntheticDefaultImports": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"moduleResolution": "NodeNext"
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"ts-node": {
		"esm": true
	},
	"rules": {}
}
